import p5 from "p5";

const sketch = (s: p5) => {
  const setup = () => {
    s.createCanvas(s.windowWidth, s.windowHeight);
  };

  const windowResized = () => {
    s.resizeCanvas(s.windowWidth, s.windowHeight);
  };

  const draw = () => {};

  s.setup = setup;
  s.windowResized = windowResized;
  s.draw = draw;
};

export default sketch;
