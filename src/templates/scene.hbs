import p5 from "p5";

const sketch = (s: p5) => {
  let time = 0;

  const setup = () => {
    s.createCanvas(s.windowWidth, s.windowHeight, s.WEBGL);
  };

  const draw = () => {
    s.background(0);

    time += 0.1;
  };

  s.setup = setup;
  s.draw = draw;
};

export default sketch;
